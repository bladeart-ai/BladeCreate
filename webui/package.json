{
  "name": "bladecreate",
  "version": "1.0.0",
  "description": "BladeCreate",
  "main": "./out/main/index.js",
  "private": true,
  "author": "https://bladeart.ai",
  "homepage": "https://bladeart.ai",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "test": "vitest",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "pnpm run typecheck:node && pnpm run typecheck:web",
    "prettier:check": "prettier --check .",
    "dev": "vite dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build-e": "pnpm run typecheck && electron-vite build",
    "dev-e": "electron-vite dev",
    "preview-e": "electron-vite preview",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "pnpm build-e && electron-builder --win --config",
    "build:mac": "pnpm build-e && electron-builder --mac --config",
    "build:linux": "pnpm build-e && electron-builder --linux --config",
    "gen-client": "rm -rf ./src/gen_client; openapi --input http://192.168.64.1:8080/openapi.json --output ./src/gen_client --client axios"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.3",
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0",
    "@hello-pangea/dnd": "^16.3.0",
    "@hookform/resolvers": "^3.3.2",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toggle": "^1.0.3",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "electron-updater": "^6.1.1",
    "form-data": "^4.0.0",
    "i18next": "^23.7.16",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.4.2",
    "jotai": "^2.5.1",
    "konva": "^9.2.3",
    "localforage": "^1.10.0",
    "lucide-react": "^0.268.0",
    "match-sorter": "^6.3.1",
    "mobx": "^6.12.0",
    "mobx-react-lite": "^4.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-i18next": "^14.0.0",
    "react-images-uploading": "^3.1.7",
    "react-konva": "^18.2.10",
    "react-resizable-panels": "^0.0.61",
    "react-router-dom": "^6.15.0",
    "sort-by": "^1.2.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "use-image": "^1.1.1",
    "uuid": "^9.0.1",
    "zod": "^3.21.4"
